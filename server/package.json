{
  "name": "suilabs-backend",
  "version": "1.0.0",
  "description": "Backend server for suilabs web",
  "main": "server",
  "scripts": {
    "test": "ENV=test nyc --reporter html --reporter text mocha --timeout=3000",
    "db:create": "babel-node ./script/createDatabase.js ./database.db",
    "db:clean": "babel-node ./script/clearDatabase.js ./database.db",
    "node-link": "ln -s ${PWD}/src node_modules/sui-server",
    "postinstall": "npm run node-link",
    "init": "echo {} ./storage/database && npm install"
  },
  "keywords": [
    "server",
    "backend"
  ],
  "author": "ShakMR",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "graphql-custom-types": "^1.2.0",
    "sha1": "^1.1.1",
    "sqlite3": "^3.1.13",
    "v8flags": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "sinon": "^4.0.1",
    "type-detect": "^4.0.3"
  }
}
