{
  "name": "suilabs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:scss:fix": "stylelint 'src/**/*.scss' --syntax scss --fix",
    "lint:js": "eslint . --ignore-path .gitignore --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint": "npm run lint:js && npm run lint:scss",
    "serve": "serve -s build",
    "start": "NODE_ENV=no-dev node ./server/bootstrap.js",
    "start:dev": "DEBUG=express:application node ./server/bootstrap.js",
    "start:client": "BROWSER=none react-scripts start src",
    "storybook": "start-storybook -p 9001",
    "test:server": "jest server/**/*.test.js",
    "test": "react-scripts test --env=jsdom",
    "precommit": "bash precommit.sh"
  },
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@babel/types": "^7.6.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.4",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.2.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-skyscanner": "^3.0.0",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "graphql": "^0.11.0",
    "ignore-styles": "^5.0.1",
    "matomo-tracker": "^2.2.1",
    "mobile-detect": "^1.4.3",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^4.4.0-beta.8",
    "react-scripts": "^3.1.2",
    "regenerator-runtime": "^0.13.3",
    "serve": "^10.1.2",
    "sitemap": "^2.2.0",
    "typeface-montserrat": "0.0.45"
  },
  "devDependencies": {
    "@storybook/react": "^4.1.18",
    "babel-jest": "^24.9.0",
    "eslint-plugin-skyscanner-dates": "^1.0.9",
    "husky": "^0.14.3",
    "jest": "^24.9.0",
    "style-loader": "^0.18.2",
    "stylelint": "^11.0.0",
    "stylelint-config-skyscanner": "^1.0.1"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
