{
  "name": "suilabs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "serve -s build",
    "start": "react-scripts start src",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "jest": "jest --config=jest.config src",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:scss:fix": "stylefmt --recursive 'src/**/*.scss'",
    "lint:js": "eslint . --ignore-path .gitignore --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint": "npm run lint:js && npm run lint:scss",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9001",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "precommit": "bash precommit.sh"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "babel-loader": "^8.0.4",
    "backpack-react-scripts": "^5.2.1",
    "graphql": "^0.11.0",
    "graphql-server-express": "^1.2.0",
    "node-sass-chokidar": "^1.3.4",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-device-detect": "^1.3.4",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.2.2",
    "serve": "^10.0.2",
    "typeface-montserrat": "0.0.45"
  },
  "devDependencies": {
    "@storybook/react": "^4.0.2",
    "bpk-component-button": "^1.6.76",
    "bpk-component-code": "^1.0.35",
    "bpk-component-grid": "^1.1.42",
    "bpk-component-text": "^1.0.33",
    "bpk-mixins": "^16.5.5",
    "bpk-stylesheets": "^3.2.79",
    "bpk-theming": "^1.1.0",
    "create-react-app-sass": "^1.1.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.13.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "husky": "^0.14.3",
    "react-test-renderer": "^16.6.0s",
    "style-loader": "^0.18.2"
  }
}
