{
  "name": "suilabs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "create:sitemap": "bash ./scripts/sitemap.sh design software about > ./public/sitemap.xml",
